    <children>
        <TextField fx:id="userName" layoutX="39.0" layoutY="31.0" promptText="Email" />
        <TextField fx:id="password" layoutX="39.0" layoutY="87.0" promptText="Password" />
        <Button layoutX="242.0" layoutY="31.0" onAction="#handleLoginClick" text="Login" />
        <Button layoutX="235.0" layoutY="87.0" onAction="#handleButtonClick" text="Sign Up" />
